{
	"name": "track-it",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"packageManager": "pnpm@8.12.1",
	"engines": {
		"node": "20.x.x"
	},
	"scripts": {
		"prepare": "husky install",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"format": "prettier --check \"{src,tests}/**/*.{js,ts,vue}\"",
		"lint": "eslint \"{src,tests}/**/*.{js,ts,vue}\"",
		"type:check": "vue-tsc --noEmit",
		"test:e2e": "cypress run --e2e",
		"test:e2e:ui": "cypress open --e2e",
		"test:component": "vitest run --config ./vitest-component.config.ts",
		"test:component:ui": "vitest --ui --config ./vitest-component.config.ts"
	},
	"dependencies": {
		"daisyui": "^4.4.19",
		"pinia": "^2.1.7",
		"vue": "^3.3.10",
		"vue-router": "^4.2.5"
	},
	"devDependencies": {
		"@pinia/testing": "^0.1.3",
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/user-event": "^14.5.1",
		"@testing-library/vue": "^8.0.1",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"@vitejs/plugin-vue": "^4.5.2",
		"@vitest/ui": "^1.0.2",
		"autoprefixer": "^10.4.16",
		"cypress": "^13.6.1",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-chai-friendly": "^0.7.2",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-vitest": "^0.3.10",
		"eslint-plugin-vue": "^9.19.2",
		"husky": "^8.0.3",
		"jsdom": "^23.0.1",
		"lint-staged": "^15.2.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"tailwindcss": "^3.3.6",
		"typescript": "^5.3.3",
		"vite": "^5.0.6",
		"vitest": "^1.0.2",
		"vue-eslint-parser": "^9.3.2",
		"vue-tsc": "^1.8.25"
	}
}
