{
  "name": "track-it",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": "20.15.0"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"{src,tests}/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint \"{src,tests}/**/*.{js,ts,vue}\" --fix",
    "type:check": "vue-tsc --noEmit",
    "test:e2e": "cypress run --e2e",
    "test:e2e:ui": "cypress open --e2e",
    "test:component": "vitest run --config ./vitest-component.config.ts",
    "test:component:ui": "vitest --ui --config ./vitest-component.config.ts"
  },
  "dependencies": {
    "daisyui": "^4.12.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@pinia/testing": "^0.1.3",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.12.0",
    "eslint": "9.13.0",
    "eslint-plugin-format": "^0.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.21"
  }
}
