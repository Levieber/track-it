{
	"name": "track-it",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"packageManager": "pnpm@8.15.1",
	"engines": {
		"node": "20.x.x"
	},
	"scripts": {
		"prepare": "husky install",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"format": "prettier --check \"{src,tests}/**/*.{js,ts,vue}\"",
		"lint": "eslint \"{src,tests}/**/*.{js,ts,vue}\"",
		"type:check": "vue-tsc --noEmit",
		"test:e2e": "cypress run --e2e",
		"test:e2e:ui": "cypress open --e2e",
		"test:component": "vitest run --config ./vitest-component.config.ts",
		"test:component:ui": "vitest --ui --config ./vitest-component.config.ts"
	},
	"dependencies": {
		"daisyui": "^4.6.1",
		"pinia": "^2.1.7",
		"vue": "^3.4.15",
		"vue-router": "^4.2.5"
	},
	"devDependencies": {
		"@pinia/testing": "^0.1.3",
		"@testing-library/jest-dom": "^6.4.1",
		"@testing-library/user-event": "^14.5.2",
		"@testing-library/vue": "^8.0.1",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@vitejs/plugin-vue": "^5.0.3",
		"@vitest/ui": "^1.2.2",
		"autoprefixer": "^10.4.17",
		"cypress": "^13.6.4",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-chai-friendly": "^0.7.4",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-vitest": "^0.3.21",
		"eslint-plugin-vue": "^9.21.1",
		"husky": "^9.0.10",
		"jsdom": "^24.0.0",
		"lint-staged": "^15.2.1",
		"postcss": "^8.4.33",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^1.2.2",
		"vue-eslint-parser": "^9.4.2",
		"vue-tsc": "^1.8.27"
	}
}
